##f$rais##
///////////////////////
//preflop attack code//
///////////////////////

//open raise
betround == 1 && call <= bblind && listOpenRaiseEP ? bblind * (4+nopponentscalling) :
f$late_raise > 0 ? ((nopponentscalling +4)*bblind) :

//reraise
betround == 1 && call > bblind  && listAllIn ? call * 3 :
betround == 1 && pt_preflop_fold_to_3bet_raischair > 0.5  && listOpenRaiseEP ? call * 3 :

//blinds
f$steal_blinds > 0 ? (3*bblind) :
f$defend_blinds > 0 ? f$defend_blinds :

///////////////////////
/////flop code/////////
///////////////////////
//with bighand we can bet for value with everything else only small bets
betround == 2 && (f$is_big_hand && call <= 3*bblind && !didrais) ? pot :

//hand can already been beaten or can be beaten at turn and river so half potbet
betround == 2 && !didrais && (nsuitedcommon == 3 || nstraightfillcommon <= 2) &&(f$top_pair_top_kicker || istwopair ) ? pot / 2 :
//less likely to be beaten but we need to make draws expensive
betround == 2 && !didrais && (nsuitedcommon <=2 && nstraightfillcommon >= 3) &&(f$top_pair_top_kicker || f$two_pair_or_more ) ? pot :
//straight & flush impossible but can be beaten
betround == 2 && !didrais && f$open_board && (f$top_pair_top_kicker || f$two_pair_or_more ) ? pot :

f$pokertracker_flop_folder > 0 ? 2* bblind :




///////////////////////
/////Turn code/////////
///////////////////////
betround == 3 && (f$is_big_hand && call <= 3*bblind && !didrais) ? pot :
betround == 3 && !didrais && (nsuitedcommon >= 3 || nstraightfillcommon <= 2) &&(f$top_pair_top_kicker || f$two_pair_or_more ) ? pot / 2 :
//less likely to be beaten but we need to make draws expensive
betround == 3 && !didrais && (nsuitedcommon <=2 && nstraightfillcommon >= 3) &&(f$top_pair_top_kicker || f$two_pair_or_more ) ? pot :
//straight & flush impossible but can be beaten
betround == 3 && !didrais && f$open_board && (f$top_pair_top_kicker || f$two_pair_or_more ) ? pot :


///////////////////////
/////River code/////////
///////////////////////
betround == 4 && (f$is_big_hand && call <= 3*bblind && !didrais) ? pot * 3 : 

//straight & flush impossible but can be beaten
betround == 4 && !didrais && f$open_board && (f$top_pair_top_kicker || f$two_pair_or_more ) ? pot : 0




















