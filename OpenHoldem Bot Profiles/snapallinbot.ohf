f$two_pair_or_more && call <= bblind * 20 ? 1 :
f$two_pair_or_more && f$open_board ? 1 :
f$three_of_a_kind_or_more && call <= bblind * 50 ? 1 :
f$three_of_a_kind_or_more && f$open_board ? 1 :
f$straight_or_more  && call <= bblind * 75 ? 1 :
f$flush_or_more ? 1 :

//if we are drawing
betround == 2 && (nsuited == 4 || nstraight == 4)
&& call <= bblind *3 ? 1 :
betround == 2 && (nsuited == 4 || nstraight == 4)
&& call <= bblind *3 ? 1 :

prwin > 0.85
? 1 : 0





























